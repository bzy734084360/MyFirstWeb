@{
    Layout = null;
    List<OrderType> data = ViewBag.data;
}
@using MVC5.Models

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Search</title>
</head>
<body>
    <h2>查询结果</h2>
    <table class="table table-condensed table-striped">
        <tr>
            <th>Id</th>
            <th>Type</th>
            <th>TypeName</th>
        </tr>
        @foreach (var item in data)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Type</td>
                <td>@item.TypeName</td>
            </tr>
        }
    </table>
</body>
</html>
